{
  "name": "alina",
  "version": "0.0.1",
  "description": "Reactive html rendering library based on native templates",
  "main": "src/Index.js",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build-dts": "tsc -p tsconfig.dts.json && rm dist/alina",
    "bundle": "rollup -c rollup-config.js src/alina.js -o dist/alina.js",
    "uglify": "uglifyjs ./dist/alina.js -o ./dist/alina.min.js",
    "build": "rm -rf dist && npm run compile && npm run build-dts && npm run bundle && npm run uglify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PFight/alina.git"
  },
  "keywords": [
    "html",
    "render"
  ],
  "author": "PFight <PFight77@gmail.com> (https://github.com/PFight)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PFight/alina/issues"
  },
  "homepage": "https://github.com/PFight/alina#readme",
  "dependencies": {
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify": "^0.1.5"
  },
  "devDependencies": {
    "uglify-js": "^3.1.6",
    "uglifyjs": "^2.4.11"
  }
}
